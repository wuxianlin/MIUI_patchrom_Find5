<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen android:title="@string/wireless_networks_settings_title"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <CheckBoxPreference android:persistent="false" android:title="@string/airplane_mode" android:key="toggle_airplane" android:disableDependentsState="true" />
    <com.android.settings_ex.SmsListPreference android:persistent="false" android:title="@string/sms_application_title" android:key="sms_application" />
    <Preference android:persistent="false" android:title="@string/voice_plus_title" android:key="voice_plus" android:summary="@string/voice_plus_summary" />
    <CheckBoxPreference android:persistent="false" android:title="@string/nfc_quick_toggle_title" android:key="toggle_nfc" android:summary="@string/nfc_quick_toggle_summary" />
    <PreferenceScreen android:title="@string/android_beam_settings_title" android:key="android_beam_settings" android:fragment="com.android.settings_ex.nfc.AndroidBeam" />
    <PreferenceScreen android:title="@string/tether_settings_title_all" android:key="tether_settings" android:fragment="com.android.settings_ex.TetherSettings" />
    <PreferenceScreen android:title="@string/vpn_settings_title" android:key="vpn_settings" android:fragment="com.android.settings_ex.vpn2.VpnSettings" />
    <Preference android:persistent="false" android:title="@string/manage_mobile_plan_title" android:key="manage_mobile_plan" />
    <PreferenceScreen android:title="@string/wimax_settings" android:key="wimax_settings">
        <intent android:targetPackage="com.android.settings_ex.wimax" android:action="android.intent.action.MAIN" android:targetClass="com.android.settings_ex.wimax.WimaxSettings" />
    </PreferenceScreen>
    <CheckBoxPreference android:persistent="false" android:title="@string/nsd_quick_toggle_title" android:key="toggle_nsd" android:summary="@string/nsd_quick_toggle_summary" />
    <PreferenceScreen android:title="@string/proxy_settings_title" android:key="proxy_settings" android:fragment="com.android.settings_ex.ProxySelector" />
    <PreferenceScreen android:title="@string/cell_broadcast_settings" android:key="cell_broadcast_settings">
        <intent android:targetPackage="com.android.cellbroadcastreceiver" android:action="android.intent.action.MAIN" android:targetClass="com.android.cellbroadcastreceiver.CellBroadcastSettings" />
    </PreferenceScreen>
</PreferenceScreen>